<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:Page2="clr-namespace:ParaTest.Page2"
             x:Class="ParaTest.Page2.MemePage">
    
    <ContentPage.BindingContext>
            <Page2:MemeViewModel></Page2:MemeViewModel>
        </ContentPage.BindingContext>
    <ContentPage.Content>
        <StackLayout>
                <Frame x:Name="HeadFrame" BackgroundColor="{DynamicResource ColorFrame}"></Frame>
                <Image Source="vlast.jpg"></Image>
                <Label Text="Ты 10 раз нажал на кнопку.Гордись собой!" TextColor ="Black"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            </StackLayout>
        </ContentPage.Content>
</ContentPage>